{
  "name": "translator-js",
  "version": "4.0.0",
  "description": "Scripts to support novel translation",
  "bin": {
    "translator-js": "bin/app.js"
  },
  "main": "src/index.js",
  "repository": "git@github.com:dino-absoluto/translator-js.git",
  "author": "Dino",
  "license": "GPL-3.0",
  "private": true,
  "scripts": {
    "standard": "standard",
    "test": "jest",
    "build": "webpack"
  },
  "dependencies": {
    "chalk": "^2.4.2",
    "cli-truncate": "^1.1.0",
    "cookie": "^0.3.1",
    "filenamify": "^3.0.0",
    "globby": "^9.1.0",
    "got": "^9.6.0",
    "jsdom": "^14.0.0",
    "make-dir": "^2.1.0",
    "mime-types": "^2.1.22",
    "yargs": "^13.2.2"
  },
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-syntax-import-meta": "^7.2.0",
    "@babel/preset-env": "^7.4.2",
    "@babel/register": "^7.4.0",
    "@types/cookie": "^0.3.2",
    "@types/got": "^9.4.1",
    "@types/jsdom": "^12.2.3",
    "awesome-typescript-loader": "^5.2.1",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.5.0",
    "babel-plugin-dynamic-import-node": "^2.2.0",
    "builtin-modules": "^3.0.0",
    "clone": "^2.1.2",
    "del": "^4.0.0",
    "gulp": "^4.0.0",
    "hasha": "^4.0.1",
    "jest": "^24.5.0",
    "json-stringify-safe": "^5.0.1",
    "rollup": "^1.7.4",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.2.2",
    "rollup-plugin-json": "^4.0.0",
    "rollup-plugin-node-resolve": "^4.0.1",
    "rollup-plugin-terser": "^4.0.4",
    "source-map-loader": "^0.2.4",
    "standard": "^12.0.1",
    "tslint": "^5.14.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.4.1",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-node-externals": "^1.7.2"
  },
  "eslintConfig": {
    "parser": "babel-eslint",
    "extends": [
      "standard"
    ]
  },
  "babel": {
    "presets": [
      [
        "@babel/env",
        {
          "targets": {
            "node": true
          }
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-syntax-dynamic-import",
      "@babel/plugin-syntax-import-meta",
      "@babel/plugin-proposal-class-properties",
      "babel-plugin-dynamic-import-node"
    ]
  },
  "jest": {
    "testEnvironment": "node",
    "roots": [
      "<rootDir>/src/",
      "<rootDir>/tests/"
    ],
    "moduleDirectories": [
      "node_modules",
      "<rootDir>/jest/mods"
    ],
    "transform": {
      "^.+\\.(js|mjs|jsx)$": "babel-jest"
    },
    "testMatch": [
      "**/?(*.)+(spec|test).(js|mjs|jsx)"
    ],
    "moduleFileExtensions": [
      "js",
      "mjs",
      "json",
      "jsx",
      "node"
    ]
  }
}
