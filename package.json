{
  "name": "translator-js",
  "version": "3.0.0",
  "description": "Scripts to support novel translation",
  "main": "src/index.js",
  "repository": "git@github.com:dino-absoluto/translator-js.git",
  "author": "Dino",
  "license": "GPL-3.0",
  "private": true,
  "scripts": {
    "standard": "standard",
    "test": "jest",
    "get": "node src/get.js"
  },
  "dependencies": {
    "chalk": "^2.3.2",
    "cookie": "^0.3.1",
    "filenamify": "^2.1.0",
    "got": "^9.2.2",
    "jsdom": "^12.1.0",
    "make-dir": "^1.3.0",
    "mime-types": "^2.1.20",
    "yargs-parser": "^11.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/preset-env": "^7.1.0",
    "@babel/register": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-jest": "^23.6.0",
    "builtin-modules": "^3.0.0",
    "del": "^3.0.0",
    "eslint": "^5.6.1",
    "gulp": "^4.0.0",
    "jest": "^23.6.0",
    "rollup": "^0.66.6",
    "rollup-plugin-babel": "^4.0.3",
    "rollup-plugin-node-resolve": "^3.4.0",
    "standard": "^12.0.1"
  },
  "eslintConfig": {
    "extends": [
      "standard"
    ]
  },
  "babel": {
    "presets": [
      [
        "@babel/env",
        {
          "useBuiltIns": "entry",
          "targets": {
            "node": true
          }
        }
      ]
    ]
  },
  "jest": {
    "roots": [
      "<rootDir>/src/",
      "<rootDir>/tests/"
    ],
    "transform": {
      "^.+\\.(js|mjs|jsx)$": "babel-jest"
    },
    "testMatch": [
      "**/?(*.)+(spec|test).(js|mjs|jsx)"
    ],
    "moduleFileExtensions": [
      "js",
      "mjs",
      "json",
      "jsx",
      "node"
    ]
  }
}
